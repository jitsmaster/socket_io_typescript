    "compilerOptions": {
		"target": "es5",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": false,
        "preserveConstEnums":false,
        "sourceMap": true
    },
    "include": [
        "**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}